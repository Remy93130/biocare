<form method="post" action="{{ path('dmp_delete', {'id': dmp.id}) }}" onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer cet élément ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ dmp.id) }}">
    <button class="btn">Supprimer</button>
</form>
